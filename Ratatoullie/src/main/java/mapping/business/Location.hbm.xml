<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="model.business">

	<class name="Location" table="location" >
	
		<id name="oid" type="java.lang.Long" column="id_location" unsaved-value="0">
				<generator class="native" />
		</id>
		<property name="latitude" type="java.lang.Double" column="latitude"/>
		<property name="longitude" type="java.lang.Double" column="longitude"/>
	
    </class>
</hibernate-mapping>