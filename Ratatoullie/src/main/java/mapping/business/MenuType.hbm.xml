<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="model.business">

	<class name="MenuType" table="menu_type" >
	
		<id name="oid" type="java.lang.Long" column="id_menu_type" unsaved-value="0">
				<generator class="native" />
		</id>
		<property name="name" type="java.lang.String" column="name" unique="true" />
		
	</class>
</hibernate-mapping>